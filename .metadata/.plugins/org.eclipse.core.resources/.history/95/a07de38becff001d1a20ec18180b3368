package oriented;

public class MotorBikeRunner {

	public static void main(String[] args) {
		MotorBike ducati = new MotorBike();
		MotorBike honda = new MotorBike();

		ducati.start();
		System.out.println(ducati.getspeed());
		honda.start();
		System.out.println(honda.getspeed());

		ducati.setSpeed(90);
		honda.setSpeed(80);

		ducati.setSpeed(20);
		honda.setSpeed(0);

	}

}
